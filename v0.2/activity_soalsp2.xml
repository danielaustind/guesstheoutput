<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.project.root.guesstheoutput.soalsp2">

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#fffa5e"
        android:textAppearance="@android:style/TextAppearance.Material.Notification"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="350dp"
        android:layout_height="296dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="#ffffff"
        android:text="\n   public class soal2{\n
        \t public static void main(String a[]){\n
        \t\t        int a=10;\n
        \t\t        int b=5;\n
        \t\t        System.out.println((a/b)+a);\n
        \t    }\n
    }"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView"
            app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="384dp"
        android:layout_height="143dp"
        android:background="@android:color/background_light"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="442dp"
        tools:layout_editor_absoluteY="400dp" />
    <TextView
        android:id="@+id/textView5"
        android:textSize="23dp"
        android:layout_width="263dp"
        android:layout_height="29dp"
        android:textColor="#000000"
        android:text="Masukkan Jawaban Anda"
        app:layout_constraintTop_toTopOf="@+id/textView"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.675"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="347dp"
        android:layout_height="53dp"
        android:textColor="#000000"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="#ffffff"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.805" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:onClick="back"
        android:layout_width="185dp"
        android:layout_height="104dp"
        android:background="#fffa5e"
        app:srcCompat="@color/colorPrimaryDark"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="442dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton12" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:onClick="next"
        android:layout_width="188dp"
        android:layout_height="104dp"
        android:background="#fffa5e"
        app:srcCompat="@color/colorPrimaryDark"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="442dp"
        android:layout_marginLeft="209dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton5" />

    <ImageView
        android:id="@+id/imageView"
        android:scaleType="fitXY"
        android:maxHeight="100dp"
        android:maxWidth="100dp"
        android:adjustViewBounds="true"
        android:background="#0000"
        android:layout_width="53dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/panahkiri"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="448dp"
        android:layout_marginRight="270dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:scaleType="fitXY"
        android:maxHeight="100dp"
        android:maxWidth="100dp"
        android:adjustViewBounds="true"
        android:background="#0000"
        android:layout_width="53dp"
        android:layout_height="63dp"
        app:srcCompat="@drawable/panahkanan"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="450dp"
        android:layout_marginLeft="271dp"
        app:layout_constraintLeft_toLeftOf="parent" />

</android.support.constraint.ConstraintLayout>
